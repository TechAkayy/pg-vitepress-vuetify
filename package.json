{
  "name": "pg-vitepress-vuetify",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://pg-vitepress-vuetify.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-vitepress-vuetify.git"
  },
  "scripts": {
    "dev": "vitepress dev  --host",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-vitepress-vuetify.netlify.app",
    "build": "vitepress build",
    "preview": "vitepress preview --open --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts ./ --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^11.0.3",
    "@vueuse/head": "^2.0.0",
    "defu": "^6.1.4",
    "lite-youtube-embed": "^0.3.3",
    "pinia": "^2.2.2",
    "vee-validate": "^4.13.2",
    "vuetify": "^3.7.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.246",
    "@nuxt/eslint-config": "^0.5.6",
    "@pinegrow/vite-plugin": "^3.0.68",
    "@pinegrow/vuetify-plugin": "^3.0.54",
    "@preact/preset-vite": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/node": "^22.5.4",
    "@unocss/preset-icons": "^0.62.3",
    "@vue/devtools": "^6.6.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "ofetch": "^1.3.4",
    "postcss": "^8.4.45",
    "preact": "^10.23.2",
    "preact-render-to-string": "^6.5.10",
    "prettier": "^3.3.3",
    "rehype-external-links": "^3.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.78.0",
    "solid-js": "^1.8.22",
    "svelte": "^4.2.19",
    "typescript": "^5.5",
    "unocss": "^0.62.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite-plugin-vue-devtools": "^7.4.4",
    "vite-plugin-vuetify": "^2.0.4",
    "vitepress": "^1.3.4",
    "vue-tsc": "^2.1"
  },
  "web-types": "web-types.json",
  "overrides": {
    "vue": "3.3",
    "vite": "4.4"
  },
  "pnpm": {
    "overrides": {
      "vue": "3.3",
      "vite": "4.4"
    }
  }
}
